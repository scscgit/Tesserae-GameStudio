<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib version="2.2"
                xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd">
    <namespace>http://www.tuke.sk/games</namespace>

    <tag>
        <tag-name>mines</tag-name>
        <component>
            <component-type>Mines</component-type>
        </component>
    </tag>

    <tag>
        <tag-name>tesserae</tag-name>
        <component>
            <component-type>Tesserae</component-type>
        </component>
        <attribute>
            <description>
                <![CDATA[Width of a Tile as displayed in html img tag.]]>
            </description>
            <name>tileWidth</name>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Height of a Tile as displayed in html img tag.]]>
            </description>
            <name>tileHeight</name>
        </attribute>
        <attribute>
            <description>
                <![CDATA[Theme used for displaying the component.]]>
            </description>
            <name>theme</name>
        </attribute>
    </tag>
</facelet-taglib>
